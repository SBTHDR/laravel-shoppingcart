{
    "name": "sparrowow/shoppingcart",
    "description": "Laravel Shoppingcart",
    "keywords": ["laravel", "shoppingcart"],
    "license": "MIT",
    "authors": [
        {
            "name": "Subrata Halder",
            "email": "sparrowowcode@gmail.com"
        }
    ],
    "require": {
      	"illuminate/support": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|5.6.*|5.7.*|5.8.*|^6|^7|^8|^9|^10|^11",
    	"illuminate/session": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|5.6.*|5.7.*|5.8.*|^6|^7|^8|^9|^10|^11",
       	"illuminate/events": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|5.6.*|5.7.*|5.8.*|^6|^7|^8|^9|^10|^11"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.0 || ~6.0 || ~7.0 || ~10.0 || ^10.5",
        "mockery/mockery": "~1.6",
        "orchestra/testbench": "~3.1"
    },
    "autoload": {
        "psr-4": {
            "Sparrowow\\Shoppingcart\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sparrowow\\Tests\\Shoppingcart\\": "tests/"
        }
    },
    "suggest": {
        "Sparrowow/notify": "Simple flash notifications for Laravel"
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Sparrowow\\Shoppingcart\\ShoppingcartServiceProvider"
            ],
            "aliases": {
                "Cart": "Sparrowow\\Shoppingcart\\Facades\\Cart"
            }
        }
    },
    "config": {
        "allow-plugins": {
            "kylekatarnls/update-helper": true
        }
    }
}
